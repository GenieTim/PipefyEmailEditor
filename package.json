{
  "name": "PipefyEmailEditor",
  "description": "Loops all your Pipefy E-Mail-Templates so you can fix them one by one.",
  "version": "0.0.0",
  "author": "Tim Bernhard @GenieTim",
  "bin": {
    "PipefyEmailEditor": "./bin/run"
  },
  "bugs": "https://github.com/GenieTim/PipefyEmailEditor/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "cli-ux": "^5.5.1",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "inquirer": "^7.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/GenieTim/PipefyEmailEditor",
  "keywords": [
    "oclif"
  ],
  "license": "GPLv3",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "PipefyEmailEditor",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "GenieTim/PipefyEmailEditor",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
