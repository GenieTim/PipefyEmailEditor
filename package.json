{
  "name": "pipefy-enhancer",
  "description": "A suit to improve various tasks in context of Pipefy.",
  "version": "1.0.1",
  "author": "Tim Bernhard @GenieTim",
  "bin": {
    "PipefyEnhancer": "./bin/run"
  },
  "bugs": "https://github.com/GenieTim/PipefyEnhancer/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "inquirer": "^7.3.3",
    "playwright": "^1.8.1",
    "twing": "^5.0.2",
    "y18n": "^5.0.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^10",
    "prettier": "^2.2.1",
    "prettier-plugin-twig-melody": "^0.4.6"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/GenieTim/PipefyEnhancer",
